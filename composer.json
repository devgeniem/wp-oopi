{
    "name": "devgeniem/wp-oopi",
    "type": "wordpress-plugin",
    "description": "OOPI is a WordPress plugin providing a developer friendly library for importing data into WordPress from external sources.",
    "authors": [
        {
            "name": "Ville Siltala",
            "email": "ville.siltala@geniem.com",
            "homepage": "https://github.com/villesiltala",
            "role": "Developer"
        },
        {
            "name": "Timi-Artturi Mäkelä",
            "email": "timi.makela@geniem.com",
            "homepage": "https://github.com/Liblastic",
            "role": "Developer"
        }
    ],
    "keywords": [
        "wordpress", "plugin", "importer", "geniem"
    ],
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "license": "GPL-3.0-or-later",
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.4 || ^8.0 || ^8.1",
        "composer/installers": "^1.0.12 || ^2"
    },
    "autoload": {
        "psr-4": {
            "Geniem\\Oopi\\": ["src/", "plugin.php"]
        }
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "10up/wp_mock": "^0.4.2",
        "phpunit/phpunit": "^9"
    }
}
